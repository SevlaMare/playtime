{
  "expo": {
    "name": "playtime",
    "slug": "playtime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon/icon.png",
    "scheme": "playtime",
    "splash": {
      "image": "./src/assets/image/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#070B24"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/icon/favicon.png"
    }
  }
}
